    "version": "0.2.0",
    "configurations": [
        

    {
        "name": "Remote Debug",
        "env": {"DISPLAY": "raspberrypi:10.0"},
        "type": "gdb",
        "request": "launch",
        "target": "./xwax",
        "arguments": "--osc  -t serato_2a -m 32 -a hw:0 -r 48000 -i import -s scan -l /media/pi/MIR_02/Contents",
        "cwd": "${workspaceRoot}",
        "valuesFormatting": "parseText"
    },
    {
        "name": "Remote Debug Dummy",
        "type": "gdb",
        "request": "launch",
        "env": {"DISPLAY": "raspberrypi:10.0"},
        "target": "xwax",
        "arguments": "--osc --dummy --dummy -i import -s scan -l /media/pi/MIR_02/Contents",
        "cwd": "${workspaceRoot}",
        "valuesFormatting": "parseText"
    }
    ]
}